{
  "name": "trainbrain",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "android:sync": "cap sync",
    "android:open": "cap open android",
    "android:update-version": "VERSION=1.0 npx trapeze run trapeze-config.yaml",
    "android:generate-icons": "npx @capacitor/assets generate --iconBackgroundColor #f5f4f6 --iconBackgroundColorDark #242430 --splashBackgroundColor #f5f4f6 --splashBackgroundColorDark #242430 --assetPath \"./src/shared/assets\" --android",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@quasar/extras": "^1.16.8",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "quasar": "^2.14.0",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@quasar/vite-plugin": "^1.6.0",
    "@rushstack/eslint-patch": "^1.6.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trapezedev/configure": "^7.0.10",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-modules-newlines": "^0.0.7",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5",
    "typescript": "5.3.2",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.22"
  }
