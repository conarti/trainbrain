{
  "name": "trainbrain",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "android:update-version": "VERSION=1.0 npx trapeze run trapeze-config.yaml",
    "android:generate-icons": "npx @capacitor/assets generate --iconBackgroundColor #f5f4f6 --iconBackgroundColorDark #242430 --splashBackgroundColor #f5f4f6 --splashBackgroundColorDark #242430 --assetPath \"./src/shared/assets\" --android",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@capacitor/android": "^5.2.3",
    "@capacitor/core": "^5.2.3",
    "@capacitor/haptics": "^5.0.6",
    "@quasar/extras": "^1.16.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.4",
    "quasar": "^2.12.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.2.3",
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@quasar/vite-plugin": "^1.4.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@trapezedev/configure": "^7.0.10",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-config-alloy": "^5.1.1",
    "eslint-plugin-filenames-simple": "^0.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-modules-newlines": "^0.0.7",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "typescript": "~5.1.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.6",
    "vitest": "^0.33.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.6"
  }
}
